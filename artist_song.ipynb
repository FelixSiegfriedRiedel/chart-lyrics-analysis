    "def get_first_artist(artist):\n",
    "    first_artist = artist\n",
    "    separators = [' Featuring ', ' & ', ' x ', ' X ', ', ', ]\n",
    "    for sep in separators:\n",
    "        if sep in artist:\n",
    "            first_artist = artist.split(sep=sep, maxsplit=1)[0]\n",
    "            return  first_artist\n",
    "    return first_artist"
   ],
